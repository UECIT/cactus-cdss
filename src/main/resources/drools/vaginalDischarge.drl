function Outcome rupturedPid(String id, Concept code) {
    return Outcome.of(id, ReferralRequest.builder()
                .id("ed-ectopic-ruptured-4h")
                .description("ED")
                .serviceRequested("ed")
                .occurrence("PT4H")
                .note("A note")
                .specialty("medicalReview&surgicalReview")
                .reason("ectopic")
                .secondaryReason(Assertion.of(code))
                .build());
}

